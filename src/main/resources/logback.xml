<configuration>

    <!--控制台输出 -->
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss} %-5level %logger{36} %L- %msg%n</pattern>
        </encoder>
    </appender>
    
    <root level="info">
        <!--appender-ref ref="FILE" /-->
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>